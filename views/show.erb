<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Chromatic Numbers Genetic Algorithm</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">
  </head>
  <body>

<nav class="navbar navbar-expand-md navbar-dark bg-dark fixed-top">
  <a class="navbar-brand" href="/">Chromatic Numbers Genetic Algorithm</a>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>



</nav>

  <main role="main" class="container" style="margin-top:6%">
    <div class="row">
    <div class="col-sm-6">
      <table class="table table-bordered table-dark table-hover">
        <h4><abbr title="Raw Data" class="initialism">Raw Data</abbr></h4>
        <thead>
          <tr>
            <th scope="col">Nodes</th>
            <th scope="col">Adjacent Vertices</th>
          </tr>
        </thead>
        <tbody>
        <% @adjacentVertices.each do |key,value| %>
          <tr>
              <td><%= "#{key}" %></td>
              <td><%= "#{value}" %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
    <div class="col-sm-6">
      <table class="table table-sm table-dark table-hover">
        <h4><abbr title="G => Green , Y => Yellow , B => Blue" class="initialism">Coloring</abbr></h4>
        <thead>
            <tr>
              <th scope="col">Nodes</th>
              <% @chromosome.each do |key,value| %>
                <th scope="col"><%= "#{key}" %></th>
              <% end %>
            </tr>
        </thead>
        <tbody>
          <tr>
              <th scope="row">Colours</th>
              <% @chromosome.each do |key,value| %>
                <td><%= "#{value}" %></td>
              <% end %>
          </tr>
        </tbody>
      </table>
      <table class="table table-sm table-dark table-hover">
        <h4><abbr title="Fitness Method" class="initialism">Fitness</abbr></h4>
        <thead>
            <tr>
              <th scope="col">Parent Population</th>
              <% @chromosomePop.each do |key,value| %>
                <th scope="col"><%= "#{key}" %></th>
              <% end %>
            </tr>
        </thead>
        <tbody>
          <th scope="row">Fitness Value</th>
          <% @fitness.each do |key,value| %>
            <td><%= "#{key}" %></td>
          <% end %>
        </tbody>
      </table>
      <table class="table table-sm table-dark table-hover">
        <h4><abbr title="Roulette Wheel" class="initialism">Roulette</abbr></h4>
        <thead>
            <tr>
              <th scope="col">First Best Value</th>
                <th scope="col"><%= "#{@roulette[0]}" %></th>
            </tr>
        </thead>
        <tbody>
          <tr>
          <th scope="row">Second Best Value</th>
            <td><%= "#{@roulette[1]}" %></td>
          </tr>
        </tbody>
      </table>
      <table class="table table-sm table-dark table-hover">
        <h4><abbr title="Crossover and Mutattion 2 times mutated!!" class="initialism">Mutated</abbr></h4>
        <thead>
            <tr>
              <th scope="col">Nodes</th>
              <!-- <% @chromosome.each do |key,value| %>
                <th scope="col"><%= "#{key}" %></th>
              <% end %> -->
            </tr>
        </thead>
        <tbody>
          <tr>
              <th scope="row">Colours</th>
              <% @crossover.each do |value| %>
                <td><%= "#{value}" %></td>
              <% end %>
          </tr>
        </tbody>
      </table>
    </div>
    </div>
  </main>

      <!-- Bootstrap core JavaScript
      ================================================== -->
      <!-- Placed at the end of the document so the pages load faster -->
      <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
      <!-- <script>window.jQuery || document.write('<script src="../../../../assets/js/vendor/jquery.min.js"><\/script>')</script> -->
      <script src="../../../../assets/js/vendor/popper.min.js"></script>
      <script src="../../../../dist/js/bootstrap.min.js"></script>
    </body>
  </html>
